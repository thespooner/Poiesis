    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Poiesis_Logo_bg.png",
        "backgroundColor": "#232355"
      }
    },
    "web": {
      "favicon": "./assets/Poiesis_Logo.png"
    }
  }
}
