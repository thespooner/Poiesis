{
  "expo": {
    "name": "poiesis",
    "slug": "poiesis",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Poiesis_Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Poiesis_Logo_bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#232355"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
